<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="PONIMPORT">
    <update id="importCardBatchUpdate"  parameterClass="java.util.HashMap">
        UPDATE t_attemp_CARD
		   SET LABEL_CN                     = #LABEL_CN#,
		       RELATED_DEVICE_CUID          = #RELATED_DEVICE_CUID#,
		       FDN                          = #FDN#,
		       CARD_KIND                    = #CARD_KIND#,
		       MODEL                        = #MODEL#,
		       RELATED_UPPER_COMPONENT_CUID = #RELATED_UPPER_COMPONENT_CUID#,
		       LAST_MODIFY_TIME             = #LAST_MODIFY_TIME#,
		       DEV_TYPE                     = #DEV_TYPE#,
		       VENDOR                       = #VENDOR#
		 WHERE CUID = #CUID#
    </update>
    <update id="updatePtpNameByCard"  parameterClass="java.util.HashMap">
        UPDATE t_attemp_PTP 
		   SET LABEL_CN = #LABEL_CN# || '.' || PORT_NO
		 WHERE RELATED_CARD_CUID = #CUID#
    </update>
    <insert id="importCardBatchInsert" parameterClass="java.util.HashMap">
		INSERT INTO t_attemp_CARD  
	    (CUID,OBJECTID,LABEL_CN,RELATED_DEVICE_CUID,FDN,CARD_KIND,MODEL,
	     CREATE_TIME,GT_VERSION,ISDELETE,LAST_MODIFY_TIME,RELATED_UPPER_COMPONENT_CUID,
	     PROJECT_STATE,IS_PERMIT_SYS_DEL,USE_TYPE,OBJECT_TYPE_CODE,LIVE_CYCLE,USE_STATE,PHOTOELECTRICITY,DEV_TYPE,VENDOR) 
	    values 
	    (#CUID#,get_object_id('CARD'),#LABEL_CN#,#RELATED_DEVICE_CUID#,#FDN#,#CARD_KIND#,#MODEL#,
	     #CREATE_TIME#,#GT_VERSION#,#ISDELETE#,#LAST_MODIFY_TIME#,#RELATED_UPPER_COMPONENT_CUID#,
	     #PROJECT_STATE#,#IS_PERMIT_SYS_DEL#,#USE_TYPE#,#OBJECT_TYPE_CODE#,#LIVE_CYCLE#,#USE_STATE#,#PHOTOELECTRICITY#,#DEV_TYPE#,#VENDOR#)
	</insert>
	<update id="importPtpBatchUpdate" parameterClass="java.util.HashMap">
		UPDATE t_attemp_PTP
		   SET LABEL_CN          = #LABEL_CN#,
		       PORT_NO           = #PORT_NO#,
		       PORT_TYPE         = #PORT_TYPE#,
		       RELATED_EMS_CUID  = #RELATED_EMS_CUID#,
		       RELATED_CARD_CUID = #RELATED_CARD_CUID#,
		       FDN               = #FDN#,
		       LAST_MODIFY_TIME  = #LAST_MODIFY_TIME#,
		       RELATED_NE_CUID   = #RELATED_NE_CUID#,
		       DEV_TYPE          = #DEV_TYPE#,
		       PORT_SUB_TYPE     = #PORT_SUB_TYPE#,
		       PORT_RATE         = #PORT_RATE#,
		       VLAN              = #VLAN#,
		       SYS_NO            = #SYS_NO#,
		       REMARK            = #REMARK#
		 WHERE CUID = #CUID#
	</update>
	<insert id="importPtpBatchInsert" parameterClass="java.util.HashMap">
		INSERT INTO t_attemp_PTP 
	    (CUID,OBJECTID,LABEL_CN,PORT_TYPE,PORT_NO,PORT_STATE,RELATED_EMS_CUID,RELATED_CARD_CUID,FDN,SYS_NO,
	      GT_VERSION,CREATE_TIME,LAST_MODIFY_TIME,PROJECT_STATE,RELATED_NE_CUID,IS_CHANNEL,TERMINATION_MODE,
	      DIRECTIONALITY,MSTP_NANFCMODE,MSTP_PORT_TYPE,IS_PERMIT_SYS_DEL,MSTP_FLOWCTRL,MSTP_ANFCMODE,
	    MSTP_LCAS_FLAG,MSTP_PPTENABLE,MSTP_EDETECT,MSTP_TAG_FLAG,MSTP_ENCAPFORMAT,MSTP_ENCAPPROTOCOL,
	    MSTP_CFLEN,MSTP_BMSGSUPPRESS,MSTP_FCSCALSEQ,LOOP_STATE,MSTP_PORTENABLE,DOMAIN,MSTP_WORKMODE,
	    MSTP_PORT_SERVICETYPE,MSTP_SCRAMBEL,LINE_BRANCH_TYPE,OBJECT_TYPE_CODE,MSTP_EXTENDEADER,LIVE_CYCLE,
	    RELATED_DISTRICT_CUID,USE_STATE,PORT_SUB_TYPE,PORT_RATE,VLAN,DEV_TYPE,REMARK)
	    values 
	    (#CUID#,get_object_id('PTP'),#LABEL_CN#,#PORT_TYPE#,#PORT_NO#,#PORT_STATE#,#RELATED_EMS_CUID#,#RELATED_CARD_CUID#,#FDN#,#SYS_NO#,
	     #GT_VERSION#,#CREATE_TIME#,#LAST_MODIFY_TIME#,#PROJECT_STATE#,#RELATED_NE_CUID#,#IS_CHANNEL#,#TERMINATION_MODE#,
	     #DIRECTIONALITY#,#MSTP_NANFCMODE#,#MSTP_PORT_TYPE#,#IS_PERMIT_SYS_DEL#,#MSTP_FLOWCTRL#,#MSTP_ANFCMODE#,
	     #MSTP_LCAS_FLAG#,#MSTP_PPTENABLE#,#MSTP_EDETECT#,#MSTP_TAG_FLAG#,#MSTP_ENCAPFORMAT#,#MSTP_ENCAPPROTOCOL#,
	     #MSTP_CFLEN#,#MSTP_BMSGSUPPRESS#,#MSTP_FCSCALSEQ#,#LOOP_STATE#,#MSTP_PORTENABLE#,#DOMAIN#,#MSTP_WORKMODE#,
	     #MSTP_PORT_SERVICETYPE#,#MSTP_SCRAMBEL#,#LINE_BRANCH_TYPE#,#OBJECT_TYPE_CODE#,#MSTP_EXTENDEADER#,#LIVE_CYCLE#,
	     #RELATED_DISTRICT_CUID#,#USE_STATE#,#PORT_SUB_TYPE#,#PORT_RATE#,#VLAN#,#DEV_TYPE#,#REMARK#)
	</insert>
</sqlMap>